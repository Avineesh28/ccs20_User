<body>
  <div id="cover"></div>
  <script>
    $(window).on("load", function () {
      $("#cover").fadeOut(200);
    });
  </script>
  <div class="navbar-fixed">
    <nav class="nav-extended" style="height: 72px;" >
      <div class="nav-wrapper">
        <a href="#" class="brand-logo left"><img src="/images/IEEE-CS_New.png" alt="logo" class="custom-logo" /></a>
        <span class="right-nav-item"><i class="fas fa-user user-icon"></i><span class="username">Aditya Menon</span></span>
      </div>      
    </nav>
  </div>
  <div class="main-content">
      <div class="first-content-container">
          <div class="domain-option row reset">
            <div class="technical domain-name col border-left border-1 ongoing">1. Technical</div>
            <div class="management domain-name col border-1">2. Management</div>
            <div class="design domain-name col border-1 not-selected">3. Design</div>
            <div class="documentation domain-name col border-right border-1 border-2">4. Documentation</div>
          </div>
          <div class="clock">
              <span class="timer"><i class="far fa-clock"></i><span class="time">HH:MM:SS</span></span>
          </div>
          
      </div>
      <div class="mobile-domain-option">
        <span>Section 1 of 4: Technical</span>
      </div>
      <div class="second-content-container">
          <div class="question-area">
              <div class="actual-question">
                  <div class="question-txt">Q1. What is the full form of IEEE?</div>

                  <!-- For Single line Subjective -->
                  <!-- <div class="option-container">
                    <label for="answer">Answer: </label>
